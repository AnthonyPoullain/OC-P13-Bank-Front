{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"airbnb",
		"prettier"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"jsx-a11y/label-has-associated-control": [
			2,
			{
				"components": [],
				"required": {
					"some": [
						"nesting",
						"id"
					]
				},
				"allowChildren": true
			}
		],
		"no-console": "off",
		"no-return-assign": "off",
		"no-param-reassign": "off",
		"react/react-in-jsx-scope": "off",
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		]
	}
}
